<%
var title = title || '';
var currentCategory = currentCategory || 'baekdu';
%>
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible"
      content="IE=edge">
<meta name="viewport"
      content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">
<title><%= title %></title>
<link rel="shortcut icon"
      href="<%= process.env.IMG_PATH %>/images/favicon.ico">
<link rel="stylesheet"
      type="text/css"
      href="<%= process.env.BASE_PATH %>/styles/plugins/jquery-ui.min.css">
<%
const fonts = [
    'SCoreDream1.woff',
    'SCoreDream1.woff2',
    'SCoreDream2.woff',
    'SCoreDream3.woff2',
    'SCoreDream4.woff',
    'SCoreDream5.woff2',
    'SCoreDream6.woff',
    'SCoreDream7.woff2',
    'SCoreDream8.woff',
    'SCoreDream9.woff2',
    'EliceDigitalBaeumRegular.woff',
    'EliceDigitalBaeumRegular.woff2',
    'EliceDigitalBaeumRegular.ttf',
    'EliceDigitalBaeumBold.woff',
    'EliceDigitalBaeumBold.woff2',
    'EliceDigitalBaeumBold.ttf',
    'SBAggroL.woff',
    'SBAggroL.otf',
    'SBAggroM.woff',
    'SBAggroM.otf',
    'SBAggroB.woff',
    'SBAggroB.otf',
]
%>
<!-- fonts -->
<% fonts.map((font, i) => { %>
<link rel="preload"
      href="<%= process.env.BASE_PATH %>/styles/fonts/<%= fonts[i] %>"
      as="font"
      type="font/<%= fonts[i].split('.')[1] %>"
      crossorigin>
<% });%>
<!-- // fonts -->
<link rel="stylesheet"
      type="text/css"
      href="<%= process.env.BASE_PATH %>/styles/koagi/<%= currentCategory %>.css">
<script src="<%= process.env.BASE_PATH %>/scripts/plugins/jquery-1.11.2.min.js"></script>
<script src="<%= process.env.BASE_PATH %>/scripts/plugins/swiper.min.js"></script>
<script src="<%= process.env.BASE_PATH %>/scripts/plugins/jquery-ui.min.js"></script>
<script src="<%= process.env.BASE_PATH %>/scripts/koagi/scripts.js"></script>
<script src="<%= process.env.BASE_PATH %>/scripts/koagi/<%= currentCategory %>.js"
        defer></script>