<%
var title = title || '';
var currentCategory = currentCategory || 'baekdu';
%>
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible"
      content="IE=edge">
<meta name="viewport"
      content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">
<title><%= title %></title>
<link rel="shortcut icon"
      href="<%= process.env.IMG_PATH %>/images/favicon.ico">
<link rel="stylesheet"
      type="text/css"
      href="<%= process.env.BASE_PATH %>/styles/plugins/jquery-ui.min.css">
<link rel="stylesheet"
      type="text/css"
      href="<%= process.env.BASE_PATH %>/styles/koagi/<%= currentCategory %>.css">
<script src="<%= process.env.BASE_PATH %>/scripts/plugins/jquery-1.11.2.min.js"></script>
<script src="<%= process.env.BASE_PATH %>/scripts/plugins/swiper.min.js"></script>
<script src="<%= process.env.BASE_PATH %>/scripts/plugins/jquery-ui.min.js"></script>
<script src="<%= process.env.BASE_PATH %>/scripts/koagi/scripts.js"></script>
<script src="<%= process.env.BASE_PATH %>/scripts/koagi/<%= currentCategory %>.js"
